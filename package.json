{
  "name": "moviesom-rn",
  "version": "3.0.0",
  "private": true,
  "devDependencies": {
    "@types/enzyme": "^3.1.12",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/enzyme-to-json": "^1.5.1",
    "@types/expo": "^27.0.6",
    "@types/expo__vector-icons": "^6.2.2",
    "@types/jest": "^23.3.1",
    "@types/numeral": "0.0.22",
    "@types/react": "^16.3.14",
    "@types/react-native": "^0.55.28",
    "@types/react-native-keyboard-spacer": "^0.4.0",
    "@types/react-navigation": "^2.0.12",
    "@types/react-redux": "^7.1.5",
    "@types/react-test-renderer": "^16.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "jest": "^22.4.4",
    "jest-expo": "^29.0.0",
    "react-native-mock-render": "0.0.27",
    "react-native-scripts-ts": "^1.15.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.3.2",
    "ts-jest": "^22.4.6",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2"
  },
  "main": "./node_modules/react-native-scripts-ts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts-ts start",
    "eject": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "publish": "exp publish",
    "build:android": "exp build:android",
    "test": "jest",
    "test-updateSnapshot": "jest --updateSnapshot"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "collectCoverage": true,
    "snapshotSerializers": [
      "./node_modules/enzyme-to-json/serializer"
    ],
    "coverageReporters": [
      "text",
      "text-summary"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+(\\.tsx?)$": "ts-jest"
    },
    "haste": {
      "defaultPlatform": "android",
      "platforms": [
        "android",
        "ios",
        "native"
      ],
      "providesModuleNodeModules": [
        "react-native"
      ]
    },
    "moduleNameMapper": {
      "credentials.json$": "<rootDir>/__mocks__/tmdb/credentials.ts"
    }
  },
  "dependencies": {
    "babel-plugin-transform-remove-console": "^6.9.4",
    "date-fns": "^1.29.0",
    "expo": "^29.0.0",
    "numeral": "^2.0.6",
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-vector-icons": "^5.0.0",
    "react-navigation": "^2.9.3",
    "react-navigation-material-bottom-tabs": "^0.3.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  }
}
